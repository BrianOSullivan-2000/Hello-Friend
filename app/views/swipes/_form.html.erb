<%= form_with(model: swipe, local: true) do |form| %>
  <% if swipe.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(swipe.errors.count, "error") %> prohibited this swipe from being saved:</h2>

      <ul>
        <% swipe.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :account_id %>
