<div class="card">
  <div class="card-header">Liked Projects</div>
  <div class="card-body">
    <% @liked_projects.each do |proj| %>
    <h5 class="card-title"><%= link_to proj.title, proj %></h5>
    <p class="card-text"><%= Account.find(proj.account_id).email %> | Mary: Hi <%= link_to "Message", Swipe.find_by(project_id: proj.id, account_id: current_account.id) %></p>
    <% end %>
  </div>
</div>
