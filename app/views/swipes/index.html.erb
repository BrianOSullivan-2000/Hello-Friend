<p id="notice"><%= notice %></p>

<h1>Swipes</h1>

<table>
  <thead>
    <tr>
      <th>Account</th>
      <th>Project</th>
      <th>Liked</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @swipes.each do |swipe| %>
      <tr>
        <td><%= swipe.account_id %></td>
        <td><%= swipe.project_id %></td>
        <td><%= swipe.liked %></td>
        <td><%= link_to 'Show', swipe %></td>
        <td><%= link_to 'Edit', edit_swipe_path(swipe) %></td>
        <td><%= link_to 'Destroy', swipe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Swipe', new_swipe_path %>
