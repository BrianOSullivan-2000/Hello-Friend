<!-- Browse Window -->
<ul id="slides">
  <% @users.each do |user| %>
  <div class="slide showing" data-id="<%= user.id %>">
    <li style="color:white"><%= user.first_name %></li>
  </div>
  <% end %>
</ul>
<button class="controls" id="previous">NO</button>
<button class="controls" id="next">YES</button>
